		cec: cec@40016000 {
			compatible = "st,stm32-cec";
			reg = <0x40016000 0x400>;
			interrupts-extended = <&intc GIC_SPI 94 IRQ_TYPE_NONE>,
					      <&exti 69 1>;
			clocks = <&rcc_clk CEC>, <&rcc_clk CK_LSE>;
			clock-names = "cec", "hdmi-cec";
