			interrupts-extended = <&intc GIC_SPI 107 IRQ_TYPE_NONE>,
					      <&intc GIC_SPI 108 IRQ_TYPE_NONE>,
					      <&exti 25 1>;
			clocks = <&rcc_clk I2C5_K>;
			resets = <&rcc_rst I2C5_R>;
			#address-cells = <1>;
			#size-cells = <0>;
			dmas = <&dmamux1 115 0x400 1>,
